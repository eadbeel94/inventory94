{
  "plugins": ["jsdoc-http-plugin"],
  "source": {
    "include": ["src"],
    "includePattern": ".js$",
    "excludePattern": "(public|extras|src/public)"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "recurse": "true",
    "destination": "./src/public/pages/doc",
    "template": "node_modules/docdash",
    "readme": "./README.jsdoc.md"
  }
}